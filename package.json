{
  "name": "@gedcom-poc/gedcom.json",
  "type": "module",
  "version": "1.0.8",
  "description": "Convert GEDCOM Files to an JSON File or an JavaScript-Object and vice versa",
  "main": "index.js",
  "scripts": {
    "demo:JSON": "node src/console.js --path 'examples/simpsons.ged'",
    "demoFile:JSON": "node src/console.js --path 'examples/simpsons.ged' --out 'test.json'",
    "demoFileWithProgress:JSON": "node src/console.js --path 'examples/royal92.ged' --out 'test.json' --showProgress"
  },
  "keywords": [
    "gedcom",
    "genealogy",
    "parser",
    "family tree",
    "family",
    "visual family tree",
    "json",
    "ged"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jisco/gedcom.json.git"
  },
  "author": "Jens Hoernecke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jisco/gedcom.json/issues"
  },
  "homepage": "https://github.com/Jisco/gedcom.json#readme",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/js-yaml": "^3.12.5",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.12",
    "@types/object-path": "^0.11.0",
    "@types/yargs": "^15.0.12"
  },
  "dependencies": {
    "dayjs": "^1.11.7",
    "deepdash-es": "^5.3.9",
    "fclone": "^1.0.11",
    "hebcal": "^2.3.2",
    "js-yaml": "^3.14.1",
    "julian-gregorian": "^1.0.0",
    "line-by-line": "^0.1.6",
    "lodash": "^4.17.21",
    "yargs": "^16.2.0"
  }
}
